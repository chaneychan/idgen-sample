	<pylon:client clientName="idgen" threadPoolSize="100"
		timeoutInMillis="1000">
		<pylon:interface id="idGenService" class="me.ele.idgen.client.IDGenService" />
	</pylon:client>

	<bean id="breakfastIdGenerator" class="me.ele.idgen.client.MemIDPool">
		<constructor-arg name="configDomain" value="n11" />
		<constructor-arg name="configKey" value="t11" />
		<constructor-arg name="allocCount" value="50" />
		<constructor-arg name="generator" ref="idGenService" />
	</bean>
	
